file_identifier "GERE";
file_extension "ge";

namespace ge.schema;

struct Frame {
  x:uint32;
  y:uint32;
}

table Sprite {
  frame_ms:uint32;
  width:uint32;
  height:uint32;
  frames:[Frame];
}

table Font {
  bytes:[uint8];
}

table Resources {
  sheet:[uint8];
  sprites:[Sprite];
  fonts:[Font];
}

root_type Resources;
