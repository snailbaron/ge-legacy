file_identifier "GERE";
file_extension "ge";

namespace ge.schema;

struct Frame {
  x:uint32;
  y:uint32;
  w:uint32;
  h:uint32;
}

table Animation {
  frame_ms:uint32;
  frames:[Frame];
}

table Font {
  bytes:[uint8];
}

table Resources {
  sheet:[uint8];
  animations:[Animation];
  fonts:[Font];
}

root_type Resources;
